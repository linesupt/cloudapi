<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mappers
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.lineying.mapper.VerifyCodeMapper">
    <!-- 查询 -->
    <select id="list" parameterType="com.lineying.entity.VerifyCodeEntity" resultType="java.util.Map">
        select ${column}
        from ${table}
        where ${where}
        order by ${sortColumn} ${sort}
    </select>
    <insert id="add" parameterType="com.lineying.entity.VerifyCodeEntity">
        insert into ${table}
                    (${column})
        value (${value})
    </insert>

</mapper>